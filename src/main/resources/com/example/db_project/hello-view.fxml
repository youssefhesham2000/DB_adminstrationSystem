<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="233.0" prefWidth="285.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/17" fx:controller="com.example.db_project.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Label alignment="TOP_LEFT" text="Email" />
   <TextField fx:id="EmailField" />
   <Label alignment="TOP_LEFT" text="Password" />
   <PasswordField fx:id="passwordField" />

    <Label fx:id="welcomeText" />
   <CheckBox fx:id="managerCheckBox" mnemonicParsing="false" text="Manager" />
   <Button fx:id="logInButton" mnemonicParsing="false" onAction="#onLogInClicked" text="Log In" />
   <Button fx:id="signUpButton" mnemonicParsing="false" onAction="#onSignUpClicked" text="Sign Up" />
</VBox>
